 import java.io.*;
 class readconversion
 {
  public static void main(string[])
  {
   BufferedReader br= new BufferedReader(new InputStreamReader(System.in));
   System.out.println("Enter a sequence\n);
   String seq = br.readLine();
   char[] dna = seq.toCharArray():
   int l = dna.length;
  
    for(int i=0; i<l; i++)
    {
     char x = dna[i];
     if(x=='T' || x=='t')
     dna[i]='u';
     }
   
   System.out.println("DNA SEQUENCE IS :" + seq);
   System.out.println("RNA SEQUENCE IS:" + String.valueof(dna));
  }
 }

