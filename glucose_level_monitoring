//glucose level
class glucose
{
 public static void main(String args[])
 {
  int i;
  i=0;
  glucose obj = new glucose;
  int.height=obj int();
  glucose obj = new glucose;
  int.weight=obj int();
  
   {
  if(i>=80 && i<=100)
  System.out.println("blood glucose is normal" +args[i]);
  else if(i>=101 && i<=125)
  System.out.println("pre-diabetic condition" +args[i]);
  else if(i>=126)
  System.out.println("diabetic condition" +args[i]);
  else 
  System.out.println("blood glucose level is low" +args[i]);
  }
 }
}
